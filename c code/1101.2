#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
	int n,i,j;
	while(scanf("%d",&n)!=EOF)
	{
		int a[11][11]={0};
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=n;j++)
			{
				char b;
				cin>>b;
				if(b=='A')
				{
					a[i][j]=1;		
				}		
				else if(b=='B')
				{
					a[i][j]=2;
				}
			}		
		}
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=n;j++)
			{
				if(a[i-1][j]!=1 && a[i+1][j]!=1 && a[i][j-1]!=1 && a[i][j+1]!=1)
				{
					a[i][j]=1;
				}
				else
				{
					a[i][j]=2;
				}
			}
		}
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=n;j++)
			{
				if(a[i][j]==1)
				{
					cout<<"A";
				}
				else
				{
					cout<<"B";
				}
			}
			cout<<endl;
		}
	}
	return 0;
}
